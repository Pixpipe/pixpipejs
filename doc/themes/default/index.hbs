<!doctype html>
<html>
<head>
  <meta charset='utf-8' />
  <title>{{options.name}} {{options.version}} | Documentation</title>
  <meta name='viewport' content='width=device-width,initial-scale=1'>
  <link href='assets/bass.css' type='text/css' rel='stylesheet' />
  <link href='assets/style.css' type='text/css' rel='stylesheet' />
  <link href='assets/github.css' type='text/css' rel='stylesheet' />
</head>
<body class='documentation'>
  <div class='container'>
    <div class='clearfix md-mxn2'>
      <div class='fixed md-show fix-3 overflow-auto max-height-100'>
        <div class='py1 px2'>
          <h3 class='mb0 no-anchor'><a href="https://github.com/jonathanlurie/pixpipejs" target="_blank"><img src="assets/pixpipe256.png" alt="{{options.name}}"/></a></h3>
          <div class='mb1'><code>{{options.version}}</code></div>
          <input
            placeholder='Filter'
            id='filter-input'
            class='col12 block field mb2 col-12'
            type='text' />
          
          <div id='toc'>
            {{#each docs}}
              <a
                href='#{{permalink}}'
                class='block bold h3 bg-pink-alpha'>
                {{name}}
              </a>
              {{#each members.static}}
                <a
                  href='#{{permalink}}'
                  class='regular block'>
                  {{name}}
                </a>
              {{/each}}
              {{#each members.instance}}
                <a
                  href='#{{permalink}}'
                  class='regular block'>
                  {{name}}
                </a>
              {{/each}}
              {{#each events}}
                <a
                  href='#{{permalink}}'
                  class='regular block'>
                  <code>{{name}}</code>
                </a>
              {{/each}}
            {{/each}}
          </div>
        </div>
      </div>
      <div class='fix-margin-3'>
        <div class='px2'>
{{#each docs ~}}<div class='py1'>{{> section}}</div>{{/each}}
        </div>
      </div>
    </div>
  </div>
  <script src='assets/anchor.js'></script>
  <script src='assets/site.js'></script>
</body>
</html>
